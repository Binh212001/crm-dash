{
  "name": "saas-vmaster",
  "version": "1.0.0",
  "description": "SaaS project with NestJS",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "start": "nest start",
    "seed:run": "env-cmd ts-node ./node_modules/typeorm-extension/bin/cli.cjs seed:run -d src/database/data-source.ts",
    "seed:create": "env-cmd ts-node ./node_modules/typeorm-extension/bin/cli.cjs seed:create -d src/database/data-source.ts",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "migration:up": "yarn typeorm migration:run",
    "migration:down": "yarn typeorm migration:revert",
    "migration:show": "yarn typeorm migration:show",
    "migration:create": "typeorm migration:create",
    "migration:generate": "yarn typeorm migration:generate --pretty",
    "pgsync:reindex": "ts-node ./scripts/pgsync-reindex.ts",
    "pgsync:generate-schema": "ts-node ./scripts/generate-schema.ts"
  },
